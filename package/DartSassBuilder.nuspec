<?xml version="1.0"?>
<package>
  <metadata>
    <id>DartSassBuilder.NET9</id>
    <version>1.1.1</version>
    <title>DartSassBuilder.NET9</title>
    <summary>Auto build task to compile .scss/.sass files to .css (.NET 9.0 version)</summary>
    <authors>Johan van Rensburg, Dean Wiseman</authors>
    <owners>Dean Wiseman</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Compile Sass files to css on project build (.NET 9.0 version).</description>
    <tags>Sass Build DartSass SassBuilder NET9</tags>
    <projectUrl>https://github.com/deanwiseman/DartSassBuilder</projectUrl>
    <repository type="git" url="https://github.com/deanwiseman/DartSassBuilder.git">
    </repository>
    <license type="expression">MIT</license>
    <icon>sass.png</icon>
  </metadata>
  <files>
    <!-- Build files -->
    <file src="build\**" target="build" />

    <!-- Tool files - DartSassBuilder executable and dependencies -->
    <file src="..\src\DartSassBuilder\bin\Release\net9.0\DartSassBuilder.dll"
      target="tool\DartSassBuilder.dll" />
    <file src="..\src\DartSassBuilder\bin\Release\net9.0\DartSassBuilder.deps.json"
      target="tool\DartSassBuilder.deps.json" />
    <file src="..\src\DartSassBuilder\bin\Release\net9.0\DartSassBuilder.runtimeconfig.json"
      target="tool\DartSassBuilder.runtimeconfig.json" />
    <file src="..\src\DartSassBuilder\bin\Release\net9.0\*.dll" target="tool"
      exclude="..\src\DartSassBuilder\bin\Release\net9.0\DartSassBuilder.dll" />
    <file src="..\src\DartSassBuilder\bin\Release\net9.0\runtimes\**" target="tool\runtimes" />

    <!-- Icon -->
    <file src="sass.png" target="sass.png" />
  </files>
</package>